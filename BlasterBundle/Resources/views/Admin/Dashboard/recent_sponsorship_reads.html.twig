<div class="row">
    <table class="table">
        <thead>
            <tr>
                <th>DJ</th>
                <th>Time Read</th>
                <th>Show Sponsor</th>
                <th>Customer</th>
                <th>Campaign</th>
        </thead>
        <tbody>
            {% for read in sponsorship_reads %}
            <tr>
                <td>{{ read.dj_initials }}</td>
                <td>{{ read.time_read|date('M j g:ia') }}</td>
                <td>{{ read.ad_name }}</td>
                <td>{{ read.customer_name }}</td>
                <td>{{ read.campaign_name }}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
